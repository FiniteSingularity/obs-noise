float3 hash33(float3 c) {
	float j = 4096.0*sin(dot(floor(c),float3(17.0, 59.4, 15.0)));
	float3 r;
	r.z = frac(512.0*j);
	j *= 0.125;
	r.x = frac(512.0*j);
	j *= 0.125;
	r.y = frac(512.0*j);
	return r;
}

float2 hash32(float3 c) {
    return hash33(c).xy;
}

float hash31(float3 c) {
    return hash33(c).x;
}

float hash11(float c) {
    return hash33(float3(c, c, c)).x;
}
